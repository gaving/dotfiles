! .Xdefaults configuration

XTerm*faceName:         Bitstream Vera Sans Mono
XTerm*faceSize:         9
XTerm*foreground:       white
XTerm*background:       black
Xterm*color0:           #000000
Xterm*color1:           #805A5A
Xterm*color2:           #598059
Xterm*color3:           #807959
Xterm*color4:           #596C80
Xterm*color5:           #80597F
Xterm*color6:           #598080
Xterm*color7:           #AAAAAA
Xterm*color8:           #555555
Xterm*color9:           #CC8F8F
Xterm*color10:          #8ECC8E
Xterm*color11:          #CCC18E
Xterm*color12:          #8EACCC
Xterm*color13:          #CC8ECA
Xterm*color14:          #8ECCCC
Xterm*color15:          white

urxvt*font:             xft:DejaVu Sans Mono:pixelsize=11:antialias=true
urxvt.geometry:         120x50
urxvt.jumpScroll:       true
urxvt.scrollBar:        false
urxvt.saveLines:        20000
urxvt.inheritPixmap:    false

urxvt.foreground:       white
urxvt.background:       black
urxvt.color0:           #000000
urxvt.color1:           #805A5A
urxvt.color2:           #598059
urxvt.color3:           #807959
urxvt.color4:           #596C80
urxvt.color5:           #80597F
urxvt.color6:           #598080
urxvt.color7:           #AAAAAA
urxvt.color8:           #555555
urxvt.color9:           #CC8F8F
urxvt.color10:          #8ECC8E
urxvt.color11:          #CCC18E
urxvt.color12:          #8EACCC
urxvt.color13:          #CC8ECA
urxvt.color14:          #8ECCCC
urxvt.color15:          white
urxvt.termName:          rxvt

urxvt.perl-ext-common:  default,matcher,macosx-clipboard
urxvt.keysym.C-M-o:     perl:matcher
urxvt.urlLauncher:      open
urxvt.matcher.button:   1
urxvt.matcher.pattern.0:    \\b(?:(?:https?|ftp|news|file):\/\/|mailto:)[ab-zA-Z0-9\-\@;\/?:&=%\$_.+!*\x27(),~#]+ [ab-zA-Z0-9\-\@;\/?:&=%\$_+!*\x27()~]
